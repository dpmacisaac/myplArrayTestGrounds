type Node{
    arr x = {1,2}
}

fun void main() {
    var y = new Node
    print(itos(y.x[0]))
    print(itos(y.x[1]))

    add(y.x,3)
    print(itos(y.x[2]))

    arr z = {y}
    print(itos(z[0].x[0]))

    arr bad = {1,2,3,4}
    print(itos(size(bad)))
    print(itos(size(z)))

    remove(y.x,0)
    print(itos(size(y.x)))


}